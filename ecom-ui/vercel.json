{
    "builds": [
      {
        "src": "dist/browser/**/*",
        "use": "@vercel/static"
      },
      {
        "src": "dist/server/main.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "dist/server/main.js"
      }
    ]
  }
  